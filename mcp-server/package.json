{
  "name": "urban-manual-mcp",
  "version": "1.0.0",
  "description": "MCP server for Urban Manual travel intelligence platform",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "urban-manual-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx index.ts",
    "inspect": "npx @modelcontextprotocol/inspector tsx index.ts",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "urban-manual",
    "travel",
    "ai"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@supabase/supabase-js": "^2.80.0",
    "@upstash/vector": "^1.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
