# Database Configuration
POSTGRES_URL=postgresql://postgres:[password]@[host]:[port]/postgres

# Optional: Supabase specific (if using Supabase client directly)
SUPABASE_URL=https://[project-id].supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Service Configuration
SERVICE_NAME=urban-manual-ml
SERVICE_VERSION=1.0.0
DEBUG=false

# ML Model Settings
MODEL_CACHE_DIR=/tmp/ml_models
MIN_INTERACTIONS=3
TOP_DESTINATIONS_FOR_FORECAST=200
FORECAST_DAYS=30

# Performance
MAX_RECOMMENDATIONS=50
API_TIMEOUT=30
