# Appfile - Apple Developer Configuration

# Apple ID for App Store Connect
apple_id(ENV["FASTLANE_USER"] || "your-apple-id@example.com")

# Bundle Identifier
app_identifier("com.travelguide.app")

# Team ID (found in Apple Developer Account)
team_id(ENV["FASTLANE_TEAM_ID"] || "YOUR_TEAM_ID")

# App Store Connect Team ID (if different from Developer Portal Team ID)
itc_team_id(ENV["FASTLANE_ITC_TEAM_ID"] || ENV["FASTLANE_TEAM_ID"] || "YOUR_TEAM_ID")
